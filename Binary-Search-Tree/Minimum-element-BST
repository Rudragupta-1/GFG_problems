/*
class Node {
    int data;
    Node left;
    Node right;
    Node(int data) {
        this.data = data;
        left = null;
        right = null;
    }
}
*/
class Solution {
    private int min=Integer.MAX_VALUE;
    // Function to find the minimum element in the given BST.
    int minValue(Node root) {
        
        if(root==null)
        {
            return -1;
        }
        min=Math.min(min,root.data);
        minValue(root.left);
        minValue(root.right);
        
        return min==Integer.MAX_VALUE?-1:min;
    }
}
